
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Maps</title>
    <style type="text/css">
        #map {
            height: 100vh;
            width: 100%;
        }
    </style>
</head>
<body>
    <div id="map"></div>
    <script>
        function initMap() {
            var options = {
                zoom: 12,
                center: {
                    lat: -23.52061876412591,
                    lng: -46.72846482518674
                }
            }
            var map = new google.maps.Map(document.getElementById('map'), options);

        }
    </script>
    <script async
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDOQiDk5rxC3E8PcVAe3eRvNG3nsl8kL40&callback=initMap">
    </script>
</body>
</html>
